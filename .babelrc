{
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    "transform-vue-jsx",
    "transform-class-properties"
  ],
  "presets": [
    "stage-0",
    "es2015"
  ]
}
